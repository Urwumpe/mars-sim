<?xml version = "1.0" encoding = "UTF-8" standalone = "yes" ?>
<template xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="settlement.xsd"
    name="Phase 2-IN" default-population="8" description="Phase 2 base for ISRO"
	number-of-robots="5" sponsor="ISRO">
    
    <building id="0" type="Lander Hab" xloc="0.0" yloc="0.0"
        facing="0.0">
        <connection-list>
            <connection id="2" xloc="-4.5" yloc="0.0" />
            <connection id="1" xloc="0.0" yloc="4.5" />
            <connection id="14" xloc="4.5" yloc="0.0" />
            <connection id="15" xloc="0.0" yloc="-4.4" />
        </connection-list>
    </building>

    <!-- This hallway connects the lander hab on the first row and the research
        hab on the second row -->
    <building id="1" type="Hallway" length="4.0" xloc="0.0"
        yloc="6.5" facing="0.0">
        <connection-list>
            <connection id="0" xloc="0.0" yloc="-2.0" />
            <connection id="6" xloc="0.0" yloc="2.0" />
        </connection-list>
    </building>

    <building id="2" type="Hallway" length="3.0" xloc="-6.0"
        yloc="0.0" facing="270.0">
        <connection-list>
            <connection id="0" xloc="0.0" yloc="1.5" />
            <connection id="3" xloc="0.0" yloc="-1.5" />
        </connection-list>
    </building>

    <building id="3" type="Lounge" xloc="-12.0" yloc="0.0"
        facing="270.0">
        <connection-list>
            <connection id="2" xloc="0.0" yloc="4.5" />
            <connection id="4" xloc="0.0" yloc="-4.5" />
        </connection-list>
    </building>

    <building id="4" type="Hallway" length="3.0" xloc="-18.0"
        yloc="0.0" facing="270.0">
        <connection-list>
            <connection id="3" xloc="0.0" yloc="1.5" />
            <connection id="5" xloc="0.0" yloc="-1.5" />
        </connection-list>
    </building>

    <building id="5" type="Residential Quarters" xloc="-24.0"
        yloc="0.0" facing="270.0">
        <connection-list>
            <connection id="4" xloc="0.0" yloc="4.5" />
            <connection id="11" xloc="-3.0" yloc="0.0" />
            <connection id="12" xloc="3.0" yloc="0.0" />
        </connection-list>
    </building>

    <building id="6" type="Machinery Hab" xloc="0.0" yloc="13.0"
        facing="0.0">
        <connection-list>
            <connection id="1" xloc="0.0" yloc="-4.5" />
            <connection id="7" xloc="-4.5" yloc="0.0" />
        </connection-list>
    </building>

    <building id="7" type="Hallway" length="3.0" xloc="-6.0"
        yloc="13.0" facing="270.0">
        <connection-list>
            <connection id="6" xloc="0.0" yloc="1.5" />
            <connection id="8" xloc="0.0" yloc="-1.5" />
        </connection-list>
    </building>

    <building id="8" type="Laboratory" xloc="-12.0" yloc="13.0"
        facing="270.0">
        <connection-list>
            <connection id="7" xloc="0.0" yloc="4.5" />
            <connection id="9" xloc="0.0" yloc="-4.5" />
        </connection-list>
    </building>

    <building id="9" type="Hallway" length="3.0" xloc="-18.0"
        yloc="13.0" facing="270.0">
        <connection-list>
            <connection id="8" xloc="0.0" yloc="1.5" />
            <connection id="10" xloc="0.0" yloc="-1.5" />
        </connection-list>
    </building>

    <building id="10" type="Inflatable Greenhouse" xloc="-24.0"
        yloc="13.0" facing="270.0">
        <connection-list>
            <connection id="9" xloc="0.0" yloc="4.5" />
            <connection id="11" xloc="3.0" yloc="0.0" />
        </connection-list>
    </building>

    <!-- This hallway connects the inflatable greenhouse on the first row
        and the inflatable greenhouse on the second row -->
    <building id="11" type="Hallway" length="7.0" xloc="-24.0"
        yloc="6.5" facing="0.0">
        <connection-list>
            <connection id="5" xloc="0.0" yloc="-3.5" />
            <connection id="10" xloc="0.0" yloc="3.5" />
        </connection-list>
    </building>

    <!-- This hallway connects the inflatable greenhouse to the garage -->
    <building id="12" type="Hallway" length="4.0" xloc="-24.0"
        yloc="-5.0" facing="0.0">
        <connection-list>
            <connection id="5" xloc="0.0" yloc="2.0" />
            <connection id="13" xloc="0.0" yloc="-2.0" />
        </connection-list>
    </building>

    <building id="13" type="Garage" xloc="-24.0" yloc="-15"
        facing="180.0">
        <connection-list>
            <connection id="12" xloc="0.0" yloc="-8.0" />
        </connection-list>
    </building>

    <!-- EVA Airlock is attached to the lander hab -->
    <building id="14" type="EVA Airlock" xloc="7.5" yloc="0.0"
        facing="0.0">
        <connection-list>
            <connection id="0" xloc="-3.0" yloc="0.0" />
        </connection-list>
    </building>

    <!-- This hallway connects the inflatable greenhouse to the fish farm -->
    <building id="15" type="Hallway" length="4.0" xloc="0"
        yloc="-6.5" facing="0.0">
        <connection-list>
            <connection id="0" xloc="0.0" yloc="2.4" />
            <connection id="16" xloc="0.0" yloc="-2.0" />
        </connection-list>
    </building>

    <!-- This fish farm connects to the inflatable greenhouse -->
    <building id="16" type="Fish Farm" xloc="0.0" yloc="-13"
        facing="180.0">
        <connection-list>
            <connection id="15" xloc="0.0" yloc="-4.4" />
        </connection-list>
    </building>


    <building type="MD1 Nuclear Reactor" xloc="500.0"
        yloc="-500" facing="0.0" />

        
    <building type="Kilopower Reactor" xloc="490.0"
        yloc="-500" facing="0.0" />
    <building type="Kilopower Reactor" xloc="490.0"
        yloc="-505" facing="0.0" />
    <building type="Kilopower Reactor" xloc="490.0"
        yloc="-510" facing="0.0" />
               
              
    <building type="Medium Battery Array" xloc="40.0"
        yloc="-40.0" facing="0.0" />        
    <building type="Solar Thermal Collector" xloc="40.0"
        yloc="-60.0" facing="0.0" />
    <building type="Solar Thermal Collector" xloc="40.0"
        yloc="-80.0" facing="0.0" />


    <building type="Medium Battery Array" xloc="60.0"
        yloc="-50.0" facing="0.0" />
    <building type="Wind Turbine" xloc="60.0"
        yloc="-60.0" facing="0.0" />
    <building type="Wind Turbine" xloc="60.0"
        yloc="-70.0" facing="0.0" />
        

    <building type="Large Sabatier Processor" xloc="0.0"
        yloc="-70.0" facing="90.0" />
    <building type="Large Sabatier Processor" xloc="0.0"
        yloc="-80.0" facing="90.0" />

    <!-- Building Packages -->
    <building-package name="Storage Bin Set 1"/>
   	<building-package name="ERV Set 1"/>

    <vehicle type="Explorer Rover" number="2" />
    <vehicle type="Light Utility Vehicle" number="1" />
    <vehicle type="Delivery Drone" number="1" />

    <equipment type="EVA Suit" number="16" />
     
    <!-- Containers-->
	<equipment type="barrel" number="40" />
	<equipment type="bag" number="40" />
	<equipment type="gas canister" number="40" />		
	<equipment type="large bag" number="40" />
	<equipment type="specimen box" number="40" />
    <equipment type="thermal bottle" number="12" />	
 	<equipment type="wheelbarrow" number="12" />
 	
	<!-- Life Support Resources -->
	<resource type="food" amount="1440" />
	<resource type="water" amount="4800" />
	<resource type="oxygen" amount="2880" />
 
    <part-package name="package 2" number="1" />
 
    <resupply>
       	<resupply-mission name="BiMonthly Delivery" manifest-name="Standard Resupply 1"
            first-arrival-time="2.5"
            frequency-sols="60" /> 
        <resupply-mission name="Annual Delivery" manifest-name="Resupply for Phase 2"
                        first-arrival-time="668"/>

    </resupply>
</template>